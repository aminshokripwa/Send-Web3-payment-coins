{
  "name": "react-eth-metamask",
  "version": "1.0.0",
  "description": "Send ETH payments using React.js anad MetaMask",
  "keywords": [
    "ethereum",
    "crypto",
    "eth",
    "metamask"
  ],
  "main": "src/index.js",
  "dependencies": {
    "ethers": "5.4.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "7.13.8",
    "typescript": "4.1.3"
  },
  "scripts": {
    "start": "SET NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "build": "SET NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}